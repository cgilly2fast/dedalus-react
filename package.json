{
  "name": "dedalus-react",
  "version": "0.1.0",
  "description": "React hooks and utilities for building chat interfaces with Dedalus",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts"
    },
    "./react": {
      "types": "./src/react/index.ts",
      "import": "./src/react/index.ts"
    },
    "./server": {
      "types": "./src/server/index.ts",
      "import": "./src/server/index.ts"
    }
  },
  "sideEffects": false,
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "dedalus-labs": "0.1.0-alpha.8"
  },
  "devDependencies": {
    "@types/react": "19.0.1",
    "typescript": "5.6.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "keywords": [
    "dedalus",
    "chat",
    "react",
    "hooks",
    "ai",
    "streaming"
  ],
  "license": "MIT"
}
